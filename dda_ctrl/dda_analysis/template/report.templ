table: 	
<TABLE WIDTH=100% CELLPADDING=4 CELLSPACING=0 border="1" bordercolor="#000000" >
{content}
</TABLE>

tableColumn: <COL WIDTH={width:d}%>

row: <TR VALIGN=TOP>{content}</TR>

column:	<TD><P ALIGN={align}>{content}</P></TD>

image: <IMG SRC="{fileName}">

